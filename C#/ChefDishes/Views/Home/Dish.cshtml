@model Dish
<a href="/addDish">Add Dish</a>
<div class="text-center">
        <a href="/">Chefs</a> | Dishes
            <h2>Here are our Dishes</h2>
    <table class="table table-striped">
        <tr>
            <th>Name</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
        @foreach (var d in ViewBag.AllDishes)
        {
            <tr>
                <td>@d.Name </td>
                <td>@d.Chef.Firstname</td>
                <td>@d.Tastiness</td>
                <td>@d.Calories</td>
            </tr>
        }
</div>